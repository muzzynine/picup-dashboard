var React = require('react');

var AdminProfile = React.createClass({
    render : function(){
	return (
		<div>
		<ul>
		<li>{this.props.adminInfo.name}</li>
		<li>{this.props.adminInfo.email}</li>
		<li>{this.props.adminInfo.thumb}</li>
		</ul>
		</div>
	);
    }
});

module.exports = AdminProfile;
